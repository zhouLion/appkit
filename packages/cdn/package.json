{
  "name": "@cotofe/appkit-cdn",
  "version": "2.0.0",
  "sideEffects": false,
  "main": "dist/wagmi.js",
  "type": "module",
  "files": [
    "dist",
    "!tsconfig.tsbuildinfo",
    "README.md"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build": "tsc && vite build",
    "watch": "tsc --watch",
    "upload": "pnpm build && pnpm publish --public",
    "serve": "http-server dist -p 8080 --cors"
  },
  "devDependencies": {
    "@wagmi/connectors": "5.7.12",
    "http-server": "14.1.1",
    "vite-plugin-node-polyfills": "0.23.0"
  },
  "peerDependencies": {
    "typescript": "5.8.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@cotofe/appkit": "workspace:*",
    "@cotofe/appkit-adapter-ethers": "workspace:*",
    "@cotofe/appkit-adapter-ethers5": "workspace:*",
    "@cotofe/appkit-adapter-solana": "workspace:*",
    "@cotofe/appkit-adapter-wagmi": "workspace:*",
    "@cotofe/appkit-polyfills": "workspace:*",
    "@wagmi/connectors": "5.8.6",
    "@wagmi/core": "2.17.3",
    "viem": "2.32.0",
    "vite": "5.4.19",
    "wagmi": "2.15.7"
  },
  "keywords": [
    "appkit",
    "wallet",
    "onboarding",
    "reown",
    "dapps",
    "web3",
    "wagmi",
    "ethereum",
    "solana",
    "bitcoin"
  ]
}
